<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TableView?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="pl.edu.wszib.student.kubalski.lab6_maven.controllers.StudentDetailsViewController">

    <Text fx:id="titleText"/>
    <VBox>
        <Button text="Go Back" onAction="#goBack" alignment="BASELINE_LEFT"/>
        <Button text="Add Grade" onAction="#addGrade" alignment="BASELINE_LEFT"/>
    </VBox>
    <TableView fx:id="studentGradesTableView"/>
</VBox>
